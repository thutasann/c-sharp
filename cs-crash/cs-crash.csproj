<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net7.0</TargetFramework>
    <RootNamespace>cs_crash</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Classes\" />
    <None Remove="Classes\Inheritance\" />
    <None Remove="Classes\ArrayOfObjects\" />
    <None Remove="Classes\MethodOverriding\" />
    <None Remove="Classes\PolyMorphism\" />
  </ItemGroup>
  <ItemGroup>
    <None Include="..\.editorconfig" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Classes\" />
    <Folder Include="Classes\Inheritance\" />
    <Folder Include="Classes\ArrayOfObjects\" />
    <Folder Include="Classes\MethodOverriding\" />
    <Folder Include="Classes\PolyMorphism\" />
  </ItemGroup>
</Project>
